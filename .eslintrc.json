{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars": [
            "error", {
                "argsIgnorePattern": "^_",
                "caughtErrors": "none"
            }],
        "consistent-return": 0
    }
}
